{
  "extends": "standard",
  "parserOptions": {
    "sourceType": "script",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": false,
      "globalReturn": false
    }
  },
  "rules": {
    "indent": ["error", 4, {
        "SwitchCase": 1
    }],
    "space-before-function-paren": ["error", {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
    }],
    "no-multiple-empty-lines": ["error", {
        "max": 2
    }]
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "M": "readonly",
    "L": "readonly",
    "S2": "readonly",
    "noUiSlider": "readonly",
